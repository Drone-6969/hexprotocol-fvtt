  "version": "1.2.3",
  "compatibility": {
    "verified": 13
  },

  "url": "https://github.com/Drone-6969/hexprotocol-fvtt",
  "manifest": "https://github.com/Drone-6969/hexprotocol-fvtt/releases/latest/download/module.json",
  "download": "https://github.com/Drone-6969/hexprotocol-fvtt/releases/download/v1.2.3/hexprotocol-v1.2.3.zip",

  "authors": [
    {
      "name": "Drone 6969",
